{
	"name": "bloom",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:o": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
		"flc": "yarn format && yarn lint && yarn check",
		"format": "prettier --write --plugin-search-dir=. ./src/routes/**/*.{js,svelte,html} ./src/lib/*.{js,svelte,html} ./src/lib/server/**/*.{js,svelte,html}",
		"lint": "eslint --cache './src/routes/**/*.{js,svelte}'"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"eslint": "^8.27.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-functional": "^4.4.1",
		"eslint-plugin-import": "latest",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "2",
		"fuzzysort": "^2.0.3"
	}
}
